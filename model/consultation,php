<?php
namespace model;

class consultation{
    private $patient_id;
    private $medecin_id;
    private $date_consultation;
    private $motif;
    private $diagnostique;
    private $traitement;
    private $note;

    public function __construct($patient_id, $medecin_id, $date_consultation, $motif, $diagnostique, $traitement, $note)
    {
        $this->patient_id = $patient_id;
        $this->medecin_id = $medecin_id;
        $this->date_consultation = $date_consultation;
        $this->motif = $motif;
        $this->diagnostique = $diagnostique;
        $this->traitement = $traitement;
        $this->note = $note;
    }
    public function getPatientId()
    {
        return $this->patient_id;
    }
    public function getMedecinId()
    {
        return $this->medecin_id;
    }
    public function getDateConsultation()
    {
        return $this->date_consultation;
    }
    public function getMotif()
    {
        return $this->motif;
    }
    public function getDiagnostique()
    {
        return $this->diagnostique;
    }
    public function getTraitement()
    {
        return $this->traitement;
    }
    public function getNote()
    {
        return $this->note;
    }
}